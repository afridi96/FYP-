<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fyp.malappdetector">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_malware"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_malware"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AboutUs"></activity>
        <activity android:name=".AboutApp" />
        <activity android:name=".MaliciousApps" />
        <activity android:name=".WhitelistAppsActivity" />
        <activity android:name=".ApplicationDetails" />
        <activity android:name=".MyApps" />
        <activity android:name=".intro" />
        <activity
            android:name=".HomeScreen"
            android:label="@string/title_activity_home_screen"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Audit"
            android:label="Audit Privacy" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name=".ScannerService"
            android:enabled="true"
            android:exported="true" />
    </application>

</manifest>